{
  "manifest_version": 3,
  "name": "Tab Counter",
  "version": "1.0",
  "description": "Counts the number of open tabs",
  "permissions": ["tabs"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Tab Counter"
  }
}
